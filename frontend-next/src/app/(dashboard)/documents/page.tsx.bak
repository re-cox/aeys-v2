    }

    try {
      console.log('[Documents] Dosya grntleniyor, ham URL:', document.fileUrl);
      const url = getFileUrl(document.fileUrl);
      console.log('[Documents] Dosya grntleniyor, ilenmi URL:', url);
      
      // URL geerli mi kontrol et
      if (url === '#') {
        console.error('[Documents] Grntleme hatas: Geersiz URL');
        toast.error("Dosya grntlenemiyor: URL bulunamad");
        return;
      }
      
      // Yeni pencerede a
      window.open(url, '_blank', 'noopener,noreferrer');
      console.log(`[Documents] Dosya grntleniyor: ${url}`);
      toast.success("Dosya yeni sekmede alyor");
    } catch (error) {
      console.error('[Documents] Dosya grntleme hatas:', error);
      toast.error("Dosya grntlenirken bir hata olutu");
    }
  };

  // Dosya indirme ileyicisi
  const handleDownloadDocument = (document: Document) => {
    // Dokmanda fileUrl yoksa hibir ey yapma
    if (!document || !document.fileUrl) {
      console.error('[Documents] ndirme hatas: fileUrl undefined veya bo', typeof document === 'object' ? document : {});
      toast.error("Dosya indirilemiyor: URL bulunamad");
      return;
    }

    try {
      console.log('[Documents] Dosya indirme balatlyor, ham URL:', document.fileUrl);
      const url = getFileUrl(document.fileUrl);
      console.log('[Documents] Dosya indirme balatlyor, ilenmi URL:', url);
      
      const fileName = document.fileName || 'dokuman';
      
      // URL geerli mi kontrol et
      if (url === '#') {
        console.error('[Documents] ndirme hatas: Geersiz URL');
        toast.error("Dosya indirilemiyor: URL bulunamad");
        return;
      }

      // Programmatik olarak indirme balants olutur
      const link = window.document.createElement('a');
      link.href = url;
      link.download = fileName;
      link.style.display = 'none';
      window.document.body.appendChild(link);
      
      console.log(`[Documents] Dosya indiriliyor: URL: ${url}, Dosya ad: ${fileName}`);
      link.click();
      
      // Temizlik
      setTimeout(() => {
        window.document.body.removeChild(link);
      }, 100);
      
      toast.success("Dosya indirme balatld");
    } catch (error) {
      console.error('[Documents] Dosya indirme hatas:', error);
      toast.error("Dosya indirilirken bir hata olutu");
    }
  };

  // Dzenleme modaln a
  const openEditSheet = (document: Document) => {
    setEditingDocument(document);
    setEditFormData({
      title: document.title || "",
      description: document.description || "",
      type: document.type,
      category: document.category,
      tags: document.tags || []
    });
  };

  // Dosya grntle ileyicisi
  const handleViewDocument = (document: Document) => {
    // Dokmanda fileUrl yoksa hibir ey yapma
    if (!document || !document.fileUrl) {
      console.error('[Documents] Grntleme hatas: fileUrl undefined veya bo', typeof document === 'object' ? document : {});
      toast.error("Dosya grntlenemiyor: URL bulunamad");
      return;
    }

    try {
      console.log('[Documents] Dosya grntleniyor, ham URL:', document.fileUrl);
      const url = getFileUrl(document.fileUrl);
      console.log('[Documents] Dosya grntleniyor, ilenmi URL:', url);
      
      // URL geerli mi kontrol et
      if (url === '#') {
        console.error('[Documents] Grntleme hatas: Geersiz URL');
        toast.error("Dosya grntlenemiyor: URL bulunamad");
        return;
      }
      
      // Yeni pencerede a
      window.open(url, '_blank', 'noopener,noreferrer');
      console.log(`[Documents] Dosya grntleniyor: ${url}`);
      toast.success("Dosya yeni sekmede alyor");
    } catch (error) {
      console.error('[Documents] Dosya grntleme hatas:', error);
      toast.error("Dosya grntlenirken bir hata olutu");
    }
  };

  // Dosya indirme ileyicisi
  return (
    <div className="container mx-auto p-4 space-y-6">
      <Card className="border-0 shadow-sm">
        <CardHeader>
          <div className="flex flex-col md:flex-row md:items-center justify-between gap-4">
                <div>
              <CardTitle className="text-2xl font-semibold">Evrak Ynetimi</CardTitle>
              <CardDescription>
                Tm belgelerinizi gvenle saklayn, grntleyin ve paylan
              </CardDescription>
            </div>
            
            <div className="flex items-center gap-2">
              <Sheet>
                <SheetTrigger asChild>
                  <Button className="gap-2">
                    <Plus size={16} />
                    Yeni Evrak Ykle
                  </Button>
                </SheetTrigger>
                <SheetContent>
                  <SheetHeader>
                    <SheetTitle>Evrak Ykleme</SheetTitle>
                    <SheetDescription>
                      Evraklarnz hzlca sisteme ykleyin ve kategorize edin.
                    </SheetDescription>
                  </SheetHeader>
                  
                  <div className="py-6 space-y-6">
                    <div className="space-y-4">
                      <Label htmlFor="file">Dosya Sein</Label>
                      <div className="grid w-full items-center gap-2">
                        <Input 
                          id="file" 
                          type="file" 
                          onChange={handleFileChange} 
                          ref={fileInputRef}
                          disabled={isUploading}
                        />
                        
                        {selectedFile && (
                          <div className="flex items-center justify-between text-sm text-muted-foreground">
                            <span>{selectedFile.name}</span>
                            <span>{formatFileSize(selectedFile.size)}</span>
                </div>
                        )}
                        
                        {uploadProgress > 0 && (
                          <div className="w-full bg-gray-200 rounded-full h-2.5 dark:bg-gray-700">
                            <div 
                              className="bg-blue-600 h-2.5 rounded-full" 
                              style={{ width: `${uploadProgress}%` }}>
                </div>
              </div>
                        )}
                      </div>
              </div>
            </div>
            
                  <SheetFooter>
                    <SheetClose asChild>
                      <Button variant="outline">ptal</Button>
                    </SheetClose>
                    <Button 
                      onClick={handleUpload} 
                      disabled={!selectedFile || isUploading}
                    >
                      {isUploading ? (
                        <>Ykleniyor...</>
                      ) : (
                        <>
                          <Upload className="w-4 h-4 mr-2" />
                          Ykle
                        </>
                      )}
                    </Button>
                  </SheetFooter>
                </SheetContent>
              </Sheet>
            </div>
          </div>
        </CardHeader>
        
        <CardContent>
          <div className="flex flex-col md:flex-row gap-4 mb-6">
            <div className="relative flex-1">
              <Search className="absolute left-3 top-1/2 transform -translate-y-1/2 text-gray-400" size={16} />
                <Input
                type="text"
                placeholder="Dosya ad, balk veya aklama ara..."
                value={searchQuery}
                onChange={(e) => setSearchQuery(e.target.value)}
                  className="pl-10"
                />
            </div>
            
            <div className="flex gap-2">
              <Select 
                value={selectedType} 
                onValueChange={(value) => setSelectedType(value as DocumentType | "ALL")}
              >
                <SelectTrigger className="w-[180px]">
                  <SelectValue placeholder="Tm Tipler" />
                </SelectTrigger>
                <SelectContent>
                  <SelectItem value="ALL">Tm Tipler</SelectItem>
                  {Object.keys(DocumentType).map((type) => (
                    <SelectItem key={type} value={type}>
                      {documentTypeConfig[type as DocumentType].label}
                    </SelectItem>
                  ))}
                </SelectContent>
              </Select>
              
              <Select 
                value={selectedCategory} 
                onValueChange={(value) => setSelectedCategory(value as DocumentCategory | "ALL")}
              >
                <SelectTrigger className="w-[180px]">
                  <SelectValue placeholder="Tm Kategoriler" />
                </SelectTrigger>
                <SelectContent>
                  <SelectItem value="ALL">Tm Kategoriler</SelectItem>
                  {Object.keys(DocumentCategory).map((category) => (
                    <SelectItem key={category} value={category}>
                      {documentCategoryConfig[category as DocumentCategory].label}
                    </SelectItem>
                  ))}
                </SelectContent>
              </Select>
            </div>
          </div>

          {loading && !documents.length ? (
            <div className="flex justify-center items-center py-12">
              <div className="animate-spin rounded-full h-10 w-10 border-b-2 border-blue-600"></div>
            </div>
          ) : error ? (
            <div className="bg-red-50 border border-red-200 text-red-700 px-4 py-3 rounded">
              <p>{error}</p>
            </div>
          ) : filteredDocuments.length === 0 ? (
            <div className="flex flex-col items-center justify-center py-12 text-center">
              <File className="h-16 w-16 text-gray-300 mb-4" />
              <h3 className="text-lg font-medium">Evrak Bulunamad</h3>
              <p className="text-muted-foreground mt-2 max-w-md">
                Arama kriterlerinize uygun bir evrak kayd bulunamad. Filtrelerinizi deitirmeyi veya yeni bir evrak yklemeyi deneyebilirsiniz.
              </p>
                    </div>
          ) : (
            <div className="overflow-x-auto">
              <Table>
                <TableHeader>
                  <TableRow>
                    <TableHead className="w-[300px]">Dosya Ad</TableHead>
                    <TableHead>Tip</TableHead>
                    <TableHead>Kategori</TableHead>
                    <TableHead>Boyut</TableHead>
                    <TableHead>Ykleyen</TableHead>
                    <TableHead>Tarih</TableHead>
                    <TableHead className="text-right">lemler</TableHead>
                  </TableRow>
                </TableHeader>
                <TableBody>
                  {filteredDocuments.map((document) => {
                    // Geerli bir DocumentType deeri mi kontrol et
                    const docType = document.type || DocumentType.OTHER;
                    // documentTypeConfig'de bu tr var m kontrol et 
                    const typeConfig = documentTypeConfig[docType] || documentTypeConfig[DocumentType.OTHER];
                    const TypeIcon = typeConfig.icon;
                    
                    // Dosya bilgilerinin kontrol
                    const fileUrl = document.fileUrl || '#';
                    const fileName = document.fileName || 'Bilinmeyen Dosya';
                    const fileTitle = document.title || fileName;
                    
                    return (
                      <TableRow key={document.id}>
                        <TableCell className="font-medium">
                          <div className="flex items-center gap-2">
                            <TypeIcon className="h-4 w-4 flex-shrink-0 text-gray-500" />
                            <div className="truncate max-w-[260px]" title={fileName}>
                              {fileTitle}
                            </div>
                          </div>
                        </TableCell>
                        <TableCell>
                          <Badge variant={typeConfig.badgeVariant}>
                            {typeConfig.label}
                          </Badge>
                        </TableCell>
                        <TableCell>
                          {documentCategoryConfig[document.category || DocumentCategory.GENERAL]?.label || "Genel"}
                        </TableCell>
                        <TableCell>
                          <div className="text-sm">
                            <span className="font-medium">{formatFileSize(document.fileSize)}</span>
                            {document.fileSize > 0 && document.fileSize < 1024 * 1024 ? 
                              <span className="text-xs text-muted-foreground ml-1">(Kk)</span> :
                              document.fileSize >= 1024 * 1024 ? 
                              <span className="text-xs text-muted-foreground ml-1">(Byk)</span> : null
                            }
                          </div>
                        </TableCell>
                        <TableCell>
                          {document.uploadedBy 
                            ? document.uploadedBy.name
                            : 'Bilinmiyor'}
                        </TableCell>
                        <TableCell>
                          {formatDate(document.uploadedAt)}
                        </TableCell>
                        <TableCell className="text-right">
                          <div className="flex justify-end gap-2">
                            {(document.fileUrl && document.fileUrl !== '') ? (
                              <>
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <Button
                                        variant="ghost" 
                                        size="icon"
                                        onClick={() => handleViewDocument(document)}
                                      >
                                        <Eye className="h-4 w-4" />
                                      </Button>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>Grntle</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                                
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <Button
                                        variant="ghost" 
                                        size="icon"
                                        onClick={() => handleDownloadDocument(document)}
                                      >
                                        <Download className="h-4 w-4" />
                                      </Button>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>ndir</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                              </>
                            ) : (
                              <>
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <Button
                                        variant="ghost" 
                                        size="icon"
                                        disabled
                                      >
                                        <Eye className="h-4 w-4 text-gray-400" />
                                      </Button>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>Dosya URL'i mevcut deil</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                                
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <Button
                                        variant="ghost" 
                                        size="icon"
                                        disabled
                                      >
                                        <Download className="h-4 w-4 text-gray-400" />
                                      </Button>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>Dosya URL'i mevcut deil</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                              </>
                            )}
                            
                            <TooltipProvider>
                              <Tooltip>
                                <TooltipTrigger asChild>
                                  <Button 
                                    variant="ghost" 
                                    size="icon" 
                                    onClick={() => openEditSheet(document)}
                                  >
                                    <Pencil className="h-4 w-4" />
                                  </Button>
                                </TooltipTrigger>
                                <TooltipContent>
                                  <p>Dzenle</p>
                                </TooltipContent>
                              </Tooltip>
                            </TooltipProvider>
                            
                            <TooltipProvider>
                              <Tooltip>
                                <TooltipTrigger asChild>
                                  <Button 
                                    variant="ghost" 
                                    size="icon"
                                    onClick={() => openDeleteDialog(document)}
                                  >
                                    <Trash2 className="h-4 w-4 text-red-500" />
                                  </Button>
                                </TooltipTrigger>
                                <TooltipContent>
                                  <p>Sil</p>
                                </TooltipContent>
                              </Tooltip>
                            </TooltipProvider>
                          </div>
                        </TableCell>
                      </TableRow>
                    );
                  })}
                </TableBody>
              </Table>
            </div>
          )}
        </CardContent>
      </Card>
      
      {/* Dzenleme Modal */}
      {editingDocument && (
        <Sheet open={!!editingDocument} onOpenChange={(open) => !open && setEditingDocument(null)}>
          <SheetContent>
            <SheetHeader>
              <SheetTitle>Evrak Dzenle</SheetTitle>
              <SheetDescription>
                Evrak bilgilerini gncelleyin ve kategorize edin.
              </SheetDescription>
            </SheetHeader>
            
            <div className="py-6 space-y-6">
              <div className="space-y-4">
                <div className="space-y-2">
                  <Label htmlFor="title">Balk</Label>
                  <Input
                    id="title"
                    name="title"
                    value={editFormData.title}
                    onChange={handleEditFormChange}
                    placeholder="Evrak bal"
                  />
                      </div>
                      
                <div className="space-y-2">
                  <Label htmlFor="description">Aklama</Label>
                  <Textarea
                    id="description"
                    name="description"
                    value={editFormData.description}
                    onChange={handleEditFormChange}
                    placeholder="Evrak hakknda aklama"
                    rows={3}
                  />
                      </div>
                      
                <div className="space-y-2">
                  <Label htmlFor="type">Tip</Label>
                  <Select value={editFormData.type} onValueChange={handleEditTypeChange}>
                    <SelectTrigger id="type">
                      <SelectValue placeholder="Tip sein" />
                    </SelectTrigger>
                    <SelectContent>
                      {Object.keys(DocumentType).map((type) => (
                        <SelectItem key={type} value={type}>
                          {documentTypeConfig[type as DocumentType].label}
                        </SelectItem>
                      ))}
                    </SelectContent>
                  </Select>
                      </div>
                      
                <div className="space-y-2">
                  <Label htmlFor="category">Kategori</Label>
                  <Select value={editFormData.category} onValueChange={handleEditCategoryChange}>
                    <SelectTrigger id="category">
                      <SelectValue placeholder="Kategori sein" />
                    </SelectTrigger>
                    <SelectContent>
                      {Object.keys(DocumentCategory).map((category) => (
                        <SelectItem key={category} value={category}>
                          {documentCategoryConfig[category as DocumentCategory].label}
                        </SelectItem>
                      ))}
                    </SelectContent>
                  </Select>
                  </div>
                  
                <div className="space-y-2">
                  <Label>Etiketler</Label>
                  <div className="flex flex-wrap gap-2 mb-2">
                    {editFormData.tags.map((tag, index) => (
                      <Badge key={index} variant="secondary" className="gap-1">
                        {tag}
                        <Button
                          variant="ghost" 
                          size="icon" 
                          className="h-4 w-4 p-0 hover:bg-transparent"
                          onClick={() => handleRemoveTag(tag)}
                        >
                          <X className="h-3 w-3" />
                        </Button>
                      </Badge>
                    ))}
                  </div>
                  <div className="flex gap-2">
                    <Input
                      value={newTag}
                      onChange={(e) => setNewTag(e.target.value)}
                      placeholder="Yeni etiket"
                      onKeyDown={(e) => e.key === 'Enter' && (e.preventDefault(), handleAddTag())}
                    />
                        <Button
                          variant="outline"
                      onClick={handleAddTag}
                      disabled={!newTag.trim()}
                    >
                      Ekle
                        </Button>
                      </div>
                    </div>
                  </div>
                </div>
            
            <SheetFooter>
              <SheetClose asChild>
                <Button variant="outline">ptal</Button>
              </SheetClose>
              <Button onClick={handleSaveEdit} disabled={loading}>
                {loading ? 'Kaydediliyor...' : 'Kaydet'}
              </Button>
            </SheetFooter>
          </SheetContent>
        </Sheet>
      )}
      
      {/* Silme Onay Dialogu */}
      <Dialog open={deleteConfirmOpen} onOpenChange={setDeleteConfirmOpen}>
        <DialogContent>
          <DialogHeader>
            <DialogTitle>Evrak Silme Onay</DialogTitle>
            <DialogDescription>
              Bu evrak silmek istediinizden emin misiniz? Bu ilem geri alnamaz.
            </DialogDescription>
          </DialogHeader>
          
          {documentToDelete && (
            <div className="py-4">
              <div className="flex items-center gap-2 mb-2">
                <FileText className="h-5 w-5 text-muted-foreground" />
                <span className="font-medium">{documentToDelete.fileName}</span>
              </div>
              <div className="text-sm text-muted-foreground">
                <p>Yklenme Tarihi: {formatDate(documentToDelete.uploadedAt)}</p>
                <p>Boyut: {formatFileSize(documentToDelete.fileSize)}</p>
              </div>
            </div>
          )}
          
          <DialogFooter>
            <Button variant="outline" onClick={() => setDeleteConfirmOpen(false)}>
              ptal
            </Button>
            <Button variant="destructive" onClick={handleDeleteDocument} disabled={loading}>
              {loading ? 'Siliniyor...' : 'Evet, Sil'}
            </Button>
          </DialogFooter>
        </DialogContent>
      </Dialog>
    </div>
  );
}