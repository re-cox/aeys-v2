        fileSize: typeof doc.fileSize === 'number' ? doc.fileSize : 0,
        uploadedById: doc.uploadedById || '',
        uploadedAt: doc.uploadedAt || ''
      };
      
      // Debug için dosya URL'lerini logla
      if (processedDoc.fileUrl) {
        console.log(`[Documents] Doküman URL (${processedDoc.id}): "${processedDoc.fileUrl}"`);
      } else {
        console.warn(`[Documents] Eksik dosya URL (${processedDoc.id})`);
      }
      
      return processedDoc;
    });

    return processedDocs;
  }, [documents, searchQuery, selectedType, selectedCategory]);
  
  // Dosya görüntüle işleyicisi
  const handleViewDocument = (document: Document) => {
    // Dokümanda fileUrl yoksa hiçbir şey yapma
    if (!document || !document.fileUrl) {
      console.error('[Documents] Görüntüleme hatası: fileUrl undefined veya boş', typeof document === 'object' ? document : {});
      toast.error("Dosya görüntülenemiyor: URL bulunamadı");
      return;
    }

    try {
      console.log('[Documents] Dosya görüntüleniyor, ham URL:', document.fileUrl);
      const url = getFileUrl(document.fileUrl);
      console.log('[Documents] Dosya görüntüleniyor, işlenmiş URL:', url);
      
      // URL geçerli mi kontrol et
      if (url === '#') {
        console.error('[Documents] Görüntüleme hatası: Geçersiz URL');
        toast.error("Dosya görüntülenemiyor: URL bulunamadı");
        return;
      }
      
      // Yeni pencerede aç
      window.open(url, '_blank', 'noopener,noreferrer');
      console.log(`[Documents] Dosya görüntüleniyor: ${url}`);
      toast.success("Dosya yeni sekmede açılıyor");
    } catch (error) {
      console.error('[Documents] Dosya görüntüleme hatası:', error);
      toast.error("Dosya görüntülenirken bir hata oluştu");
    }
  };

  // Dosya indirme işleyicisi
  const handleDownloadDocument = (document: Document) => {
    // Dokümanda fileUrl yoksa hiçbir şey yapma
    if (!document || !document.fileUrl) {
      console.error('[Documents] İndirme hatası: fileUrl undefined veya boş', typeof document === 'object' ? document : {});
      toast.error("Dosya indirilemiyor: URL bulunamadı");
      return;
    }

    try {
      console.log('[Documents] Dosya indirme başlatılıyor, ham URL:', document.fileUrl);
      const url = getFileUrl(document.fileUrl);
      console.log('[Documents] Dosya indirme başlatılıyor, işlenmiş URL:', url);
      
      const fileName = document.fileName || 'dokuman';
      
      // URL geçerli mi kontrol et
      if (url === '#') {
        console.error('[Documents] İndirme hatası: Geçersiz URL');
        toast.error("Dosya indirilemiyor: URL bulunamadı");
        return;
      }

      // Programmatik olarak indirme bağlantısı oluştur
      const link = window.document.createElement('a');
      link.href = url;
      link.download = fileName;
      link.style.display = 'none';
      window.document.body.appendChild(link);
      
      console.log(`[Documents] Dosya indiriliyor: URL: ${url}, Dosya adı: ${fileName}`);
      link.click();
      
      // Temizlik
      setTimeout(() => {
        window.document.body.removeChild(link);
      }, 100);
      
      toast.success("Dosya indirme başlatıldı");
    } catch (error) {
      console.error('[Documents] Dosya indirme hatası:', error);
      toast.error("Dosya indirilirken bir hata oluştu");
    }
  };

  // Düzenleme modalını aç
  const openEditSheet = (document: Document) => {
    setEditingDocument(document);
    setEditFormData({
      title: document.title || "",
      description: document.description || "",
      type: document.type,
      category: document.category,
      tags: document.tags || []
    });
  };

  // Dosya görüntüle işleyicisi
  const handleViewDocument = (document: Document) => {
    // Dokümanda fileUrl yoksa hiçbir şey yapma
    if (!document || !document.fileUrl) {
      console.error('[Documents] Görüntüleme hatası: fileUrl undefined veya boş', typeof document === 'object' ? document : {});
      toast.error("Dosya görüntülenemiyor: URL bulunamadı");
      return;
    }

    try {
      console.log('[Documents] Dosya görüntüleniyor, ham URL:', document.fileUrl);
      const url = getFileUrl(document.fileUrl);
      console.log('[Documents] Dosya görüntüleniyor, işlenmiş URL:', url);
      
      // URL geçerli mi kontrol et
      if (url === '#') {
        console.error('[Documents] Görüntüleme hatası: Geçersiz URL');
        toast.error("Dosya görüntülenemiyor: URL bulunamadı");
        return;
      }
      
      // Yeni pencerede aç
      window.open(url, '_blank', 'noopener,noreferrer');
      console.log(`[Documents] Dosya görüntüleniyor: ${url}`);
      toast.success("Dosya yeni sekmede açılıyor");
    } catch (error) {
      console.error('[Documents] Dosya görüntüleme hatası:', error);
      toast.error("Dosya görüntülenirken bir hata oluştu");
    }
  };

  // Dosya indirme işleyicisi
  return (
    <div className="container mx-auto p-4 space-y-6">
      <Card className="border-0 shadow-sm">
        <CardHeader>
          <div className="flex flex-col md:flex-row md:items-center justify-between gap-4">
                <div>
              <CardTitle className="text-2xl font-semibold">Evrak Yönetimi</CardTitle>
              <CardDescription>
                Tüm belgelerinizi güvenle saklayın, görüntüleyin ve paylaşın
              </CardDescription>
            </div>
            
            <div className="flex items-center gap-2">
              <Sheet>
                <SheetTrigger asChild>
                  <Button className="gap-2">
                    <Plus size={16} />
                    Yeni Evrak Yükle
                  </Button>
                </SheetTrigger>
                <SheetContent>
                  <SheetHeader>
                    <SheetTitle>Evrak Yükleme</SheetTitle>
                    <SheetDescription>
                      Evraklarınızı hızlıca sisteme yükleyin ve kategorize edin.
                    </SheetDescription>
                  </SheetHeader>
                  
                  <div className="py-6 space-y-6">
                    <div className="space-y-4">
                      <Label htmlFor="file">Dosya Seçin</Label>
                      <div className="grid w-full items-center gap-2">
                        <Input 
                          id="file" 
                          type="file" 
                          onChange={handleFileChange} 
                          ref={fileInputRef}
                          disabled={isUploading}
                        />
                        
                        {selectedFile && (
                          <div className="flex items-center justify-between text-sm text-muted-foreground">
                            <span>{selectedFile.name}</span>
                            <span>{formatFileSize(selectedFile.size)}</span>
                </div>
                        )}
                        
                        {uploadProgress > 0 && (
                          <div className="w-full bg-gray-200 rounded-full h-2.5 dark:bg-gray-700">
                            <div 
                              className="bg-blue-600 h-2.5 rounded-full" 
                              style={{ width: `${uploadProgress}%` }}>
                </div>
              </div>
                        )}
                      </div>
              </div>
            </div>
            
                  <SheetFooter>
                    <SheetClose asChild>
                      <Button variant="outline">İptal</Button>
                    </SheetClose>
                    <Button 
                      onClick={handleUpload} 
                      disabled={!selectedFile || isUploading}
                    >
                      {isUploading ? (
                        <>Yükleniyor...</>
                      ) : (
                        <>
                          <Upload className="w-4 h-4 mr-2" />
                          Yükle
                        </>
                      )}
                    </Button>
                  </SheetFooter>
                </SheetContent>
              </Sheet>
            </div>
          </div>
        </CardHeader>
        
        <CardContent>
          <div className="flex flex-col md:flex-row gap-4 mb-6">
            <div className="relative flex-1">
              <Search className="absolute left-3 top-1/2 transform -translate-y-1/2 text-gray-400" size={16} />
                <Input
                type="text"
                placeholder="Dosya adı, başlık veya açıklama ara..."
                value={searchQuery}
                onChange={(e) => setSearchQuery(e.target.value)}
                  className="pl-10"
                />
            </div>
            
            <div className="flex gap-2">
              <Select 
                value={selectedType} 
                onValueChange={(value) => setSelectedType(value as DocumentType | "ALL")}
              >
                <SelectTrigger className="w-[180px]">
                  <SelectValue placeholder="Tüm Tipler" />
                </SelectTrigger>
                <SelectContent>
                  <SelectItem value="ALL">Tüm Tipler</SelectItem>
                  {Object.keys(DocumentType).map((type) => (
                    <SelectItem key={type} value={type}>
                      {documentTypeConfig[type as DocumentType].label}
                    </SelectItem>
                  ))}
                </SelectContent>
              </Select>
              
              <Select 
                value={selectedCategory} 
                onValueChange={(value) => setSelectedCategory(value as DocumentCategory | "ALL")}
              >
                <SelectTrigger className="w-[180px]">
                  <SelectValue placeholder="Tüm Kategoriler" />
                </SelectTrigger>
                <SelectContent>
                  <SelectItem value="ALL">Tüm Kategoriler</SelectItem>
                  {Object.keys(DocumentCategory).map((category) => (
                    <SelectItem key={category} value={category}>
                      {documentCategoryConfig[category as DocumentCategory].label}
                    </SelectItem>
                  ))}
                </SelectContent>
              </Select>
            </div>
          </div>

          {loading && !documents.length ? (
            <div className="flex justify-center items-center py-12">
              <div className="animate-spin rounded-full h-10 w-10 border-b-2 border-blue-600"></div>
            </div>
          ) : error ? (
            <div className="bg-red-50 border border-red-200 text-red-700 px-4 py-3 rounded">
              <p>{error}</p>
            </div>
          ) : filteredDocuments.length === 0 ? (
            <div className="flex flex-col items-center justify-center py-12 text-center">
              <File className="h-16 w-16 text-gray-300 mb-4" />
              <h3 className="text-lg font-medium">Evrak Bulunamadı</h3>
              <p className="text-muted-foreground mt-2 max-w-md">
                Arama kriterlerinize uygun bir evrak kaydı bulunamadı. Filtrelerinizi değiştirmeyi veya yeni bir evrak yüklemeyi deneyebilirsiniz.
              </p>
                    </div>
          ) : (
            <div className="overflow-x-auto">
              <Table>
                <TableHeader>
                  <TableRow>
                    <TableHead className="w-[300px]">Dosya Adı</TableHead>
                    <TableHead>Tip</TableHead>
                    <TableHead>Kategori</TableHead>
                    <TableHead>Boyut</TableHead>
                    <TableHead>Yükleyen</TableHead>
                    <TableHead>Tarih</TableHead>
                    <TableHead className="text-right">İşlemler</TableHead>
                  </TableRow>
                </TableHeader>
                <TableBody>
                  {filteredDocuments.map((document) => {
                    // Geçerli bir DocumentType değeri mi kontrol et
                    const docType = document.type || DocumentType.OTHER;
                    // documentTypeConfig'de bu tür var mı kontrol et 
                    const typeConfig = documentTypeConfig[docType] || documentTypeConfig[DocumentType.OTHER];
                    const TypeIcon = typeConfig.icon;
                    
                    // Dosya bilgilerinin kontrolü
                    const fileUrl = document.fileUrl || '#';
                    const fileName = document.fileName || 'Bilinmeyen Dosya';
                    const fileTitle = document.title || fileName;
                    
                    return (
                      <TableRow key={document.id}>
                        <TableCell className="font-medium">
                          <div className="flex items-center gap-2">
                            <TypeIcon className="h-4 w-4 flex-shrink-0 text-gray-500" />
                            <div className="truncate max-w-[260px]" title={fileName}>
                              {fileTitle}
                            </div>
                          </div>
                        </TableCell>
                        <TableCell>
                          <Badge variant={typeConfig.badgeVariant}>
                            {typeConfig.label}
                          </Badge>
                        </TableCell>
                        <TableCell>
                          {documentCategoryConfig[document.category || DocumentCategory.GENERAL]?.label || "Genel"}
                        </TableCell>
                        <TableCell>
                          <div className="text-sm">
                            <span className="font-medium">{formatFileSize(document.fileSize)}</span>
                            {document.fileSize > 0 && document.fileSize < 1024 * 1024 ? 
                              <span className="text-xs text-muted-foreground ml-1">(Küçük)</span> :
                              document.fileSize >= 1024 * 1024 ? 
                              <span className="text-xs text-muted-foreground ml-1">(Büyük)</span> : null
                            }
                          </div>
                        </TableCell>
                        <TableCell>
                          {document.uploadedBy 
                            ? document.uploadedBy.name
                            : 'Bilinmiyor'}
                        </TableCell>
                        <TableCell>
                          {formatDate(document.uploadedAt)}
                        </TableCell>
                        <TableCell className="text-right">
                          <div className="flex justify-end gap-2">
                            {(document.fileUrl && document.fileUrl !== '') ? (
                              <>
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <Button
                                        variant="ghost" 
                                        size="icon"
                                        onClick={() => handleViewDocument(document)}
                                      >
                                        <Eye className="h-4 w-4" />
                                      </Button>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>Görüntüle</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                                
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <Button
                                        variant="ghost" 
                                        size="icon"
                                        onClick={() => handleDownloadDocument(document)}
                                      >
                                        <Download className="h-4 w-4" />
                                      </Button>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>İndir</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                              </>
                            ) : (
                              <>
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <Button
                                        variant="ghost" 
                                        size="icon"
                                        disabled
                                      >
                                        <Eye className="h-4 w-4 text-gray-400" />
                                      </Button>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>Dosya URL'i mevcut değil</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                                
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <Button
                                        variant="ghost" 
                                        size="icon"
                                        disabled
                                      >
                                        <Download className="h-4 w-4 text-gray-400" />
                                      </Button>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>Dosya URL'i mevcut değil</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                              </>
                            )}
                            
                            <TooltipProvider>
                              <Tooltip>
                                <TooltipTrigger asChild>
                                  <Button 
                                    variant="ghost" 
                                    size="icon" 
                                    onClick={() => openEditSheet(document)}
                                  >
                                    <Pencil className="h-4 w-4" />
                                  </Button>
                                </TooltipTrigger>
                                <TooltipContent>
                                  <p>Düzenle</p>
                                </TooltipContent>
                              </Tooltip>
                            </TooltipProvider>
                            
                            <TooltipProvider>
                              <Tooltip>
                                <TooltipTrigger asChild>
                                  <Button 
                                    variant="ghost" 
                                    size="icon"
                                    onClick={() => openDeleteDialog(document)}
                                  >
                                    <Trash2 className="h-4 w-4 text-red-500" />
                                  </Button>
                                </TooltipTrigger>
                                <TooltipContent>
                                  <p>Sil</p>
                                </TooltipContent>
                              </Tooltip>
                            </TooltipProvider>
                          </div>
                        </TableCell>
                      </TableRow>
                    );
                  })}
                </TableBody>
              </Table>
            </div>
          )}
        </CardContent>
      </Card>
      
      {/* Düzenleme Modalı */}
      {editingDocument && (
        <Sheet open={!!editingDocument} onOpenChange={(open) => !open && setEditingDocument(null)}>
          <SheetContent>
            <SheetHeader>
              <SheetTitle>Evrakı Düzenle</SheetTitle>
              <SheetDescription>
                Evrak bilgilerini güncelleyin ve kategorize edin.
              </SheetDescription>
            </SheetHeader>
            
            <div className="py-6 space-y-6">
              <div className="space-y-4">
                <div className="space-y-2">
                  <Label htmlFor="title">Başlık</Label>
                  <Input
                    id="title"
                    name="title"
                    value={editFormData.title}
                    onChange={handleEditFormChange}
                    placeholder="Evrak başlığı"
                  />
                      </div>
                      
                <div className="space-y-2">
                  <Label htmlFor="description">Açıklama</Label>
                  <Textarea
                    id="description"
                    name="description"
                    value={editFormData.description}
                    onChange={handleEditFormChange}
                    placeholder="Evrak hakkında açıklama"
                    rows={3}
                  />
                      </div>
                      
                <div className="space-y-2">
                  <Label htmlFor="type">Tip</Label>
                  <Select value={editFormData.type} onValueChange={handleEditTypeChange}>
                    <SelectTrigger id="type">
                      <SelectValue placeholder="Tip seçin" />
                    </SelectTrigger>
                    <SelectContent>
                      {Object.keys(DocumentType).map((type) => (
                        <SelectItem key={type} value={type}>
                          {documentTypeConfig[type as DocumentType].label}
                        </SelectItem>
                      ))}
                    </SelectContent>
                  </Select>
                      </div>
                      
                <div className="space-y-2">
                  <Label htmlFor="category">Kategori</Label>
                  <Select value={editFormData.category} onValueChange={handleEditCategoryChange}>
                    <SelectTrigger id="category">
                      <SelectValue placeholder="Kategori seçin" />
                    </SelectTrigger>
                    <SelectContent>
                      {Object.keys(DocumentCategory).map((category) => (
                        <SelectItem key={category} value={category}>
                          {documentCategoryConfig[category as DocumentCategory].label}
                        </SelectItem>
                      ))}
                    </SelectContent>
                  </Select>
                  </div>
                  
                <div className="space-y-2">
                  <Label>Etiketler</Label>
                  <div className="flex flex-wrap gap-2 mb-2">
                    {editFormData.tags.map((tag, index) => (
                      <Badge key={index} variant="secondary" className="gap-1">
                        {tag}
                        <Button
                          variant="ghost" 
                          size="icon" 
                          className="h-4 w-4 p-0 hover:bg-transparent"
                          onClick={() => handleRemoveTag(tag)}
                        >
                          <X className="h-3 w-3" />
                        </Button>
                      </Badge>
                    ))}
                  </div>
                  <div className="flex gap-2">
                    <Input
                      value={newTag}
                      onChange={(e) => setNewTag(e.target.value)}
                      placeholder="Yeni etiket"
                      onKeyDown={(e) => e.key === 'Enter' && (e.preventDefault(), handleAddTag())}
                    />
                        <Button
                          variant="outline"
                      onClick={handleAddTag}
                      disabled={!newTag.trim()}
                    >
                      Ekle
                        </Button>
                      </div>
                    </div>
                  </div>
                </div>
            
            <SheetFooter>
              <SheetClose asChild>
                <Button variant="outline">İptal</Button>
              </SheetClose>
              <Button onClick={handleSaveEdit} disabled={loading}>
                {loading ? 'Kaydediliyor...' : 'Kaydet'}
              </Button>
            </SheetFooter>
          </SheetContent>
        </Sheet>
      )}
      
      {/* Silme Onay Dialogu */}
      <Dialog open={deleteConfirmOpen} onOpenChange={setDeleteConfirmOpen}>
        <DialogContent>
          <DialogHeader>
            <DialogTitle>Evrak Silme Onayı</DialogTitle>
            <DialogDescription>
              Bu evrakı silmek istediğinizden emin misiniz? Bu işlem geri alınamaz.
            </DialogDescription>
          </DialogHeader>
          
          {documentToDelete && (
            <div className="py-4">
              <div className="flex items-center gap-2 mb-2">
                <FileText className="h-5 w-5 text-muted-foreground" />
                <span className="font-medium">{documentToDelete.fileName}</span>
              </div>
              <div className="text-sm text-muted-foreground">
                <p>Yüklenme Tarihi: {formatDate(documentToDelete.uploadedAt)}</p>
                <p>Boyut: {formatFileSize(documentToDelete.fileSize)}</p>
              </div>
            </div>
          )}
          
          <DialogFooter>
            <Button variant="outline" onClick={() => setDeleteConfirmOpen(false)}>
              İptal
            </Button>
            <Button variant="destructive" onClick={handleDeleteDocument} disabled={loading}>
              {loading ? 'Siliniyor...' : 'Evet, Sil'}
            </Button>
          </DialogFooter>
        </DialogContent>
      </Dialog>
    </div>
  );
}